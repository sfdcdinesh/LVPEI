<aura:component controller="TissueEvaluationSave" implements="force:appHostable,lightning:actionOverride,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    
    <aura:attribute name="imageholding" type="blob"/>
    <aura:attribute name="imageholding1" type="blob"/>
    <aura:attribute name="booleanvalue" type="boolean" default="false"/>
    <aura:attribute name="booleanvalue1" type="boolean" default="false"/>
    <aura:attribute name="booleanvalue2" type="boolean" default="false"/>
    <aura:attribute name="booleanvalue3" type="boolean" default="false"/>
    <aura:attribute name="booleanvalue4" type="boolean" default="false"/>
    <aura:attribute name="TissRecoveryDate" type="Datetime" default="false"/>
    <aura:attribute name="datetime" type="Datetime" />
    <aura:attribute name="imgurl" type="String" default="/sfc/servlet.shepherd/version/download/00P6F00002NkTpwUAF"/>
    <aura:attribute name="DonorCon" type="sObject" default="{}"/> 
    <aura:attribute name="DonorCon1" type="sObject" default="{}"/>
    <aura:attribute name="DonorCon2" type="sObject" default="{}"/>
    <aura:attribute name="DonorCon3" type="sObject" default="{}"/>
    <aura:attribute name="DonorCon4" type="sObject" default="{}"/>
    <aura:attribute name="DonorCon5" type="sObject" default="{}"/>
    <aura:attribute name="DonorCon6" type="sObject" default="{}"/>
    <aura:attribute name="DonorCon7" type="sObject" default="{}"/>
    <aura:attribute name="DonorCon8" type="sObject" default="{}"/>
    <aura:attribute name="tisEval" type="Tissue_Evaluation__c" default="{'sobjectType': 'Tissue_Evaluation__c'}" /> 
    <aura:attribute name="spinner" type="Boolean" default="false"/>
    <aura:attribute name="display" type="Boolean" default="false"/>
    <aura:attribute name="display1" type="Boolean" default="false"/>
    <aura:attribute name="display2" type="Boolean" default="false"/>
    <aura:attribute name="display3" type="Boolean" default="false"/>
    <aura:attribute name="display4" type="Boolean" default="false"/>
    <aura:attribute name="display5" type="Boolean" default="false"/>
    <aura:attribute name="display6" type="Boolean" default="false"/>
    <aura:attribute name="display7" type="Boolean" default="false"/>
    <aura:attribute name="display8" type="Boolean" default="false"/>
    <aura:attribute name="display10" type="Boolean" default="false"/>
    <aura:attribute name="profileName" type="string" default="false"/>
    <!-- APPROVAL OUTCOME RENDERED ATTRIBUTES -->
    <aura:attribute name="display0" type="Boolean" default="false"/>
    <aura:attribute name="display9" type="Boolean" default="false"/>
    <aura:attribute name="display11" type="Boolean" default="false"/>
    <aura:attribute name="display12" type="Boolean" default="false"/>
    <aura:attribute name="display13" type="Boolean" default="false"/>
    <aura:attribute name="display14" type="Boolean" default="false"/>
    <aura:attribute name="display15" type="Boolean" default="false"/>
    <aura:attribute name="display16" type="Boolean" default="false"/>
    <aura:attribute name="display17" type="Boolean" default="false"/>
    <aura:attribute name="display18" type="Boolean" default="false"/>
    <aura:attribute name="slough" type="Boolean" default="false"/>
    <aura:attribute name="opaciti" type="Boolean" default="false"/>
    <aura:attribute name="opaciti2" type="Boolean" default="false"/>
    <aura:attribute name="fold" type="Boolean" default="false"/>
    <aura:attribute name="desc" type="Boolean" default="false"/>
    <aura:attribute name="stressline" type="Boolean" default="false"/>
    <aura:attribute name="ProcedureNo" type="Boolean" default="false"/>
    <aura:attribute name="DebrisYes" type="Boolean" default="false"/>
    <aura:attribute name="EndoDefectYes" type="Boolean" default="false"/>
    
    <aura:attribute name="fileNamespecEval" type="String" default="No File Selected.." />
    <aura:attribute name="fileNamepostCut" type="String" default="No File Selected.." />
    <aura:attribute name="firstimg" type="boolean" default="false"/>
    <aura:attribute name="imageName" type="string"/>
    <aura:attribute name="imageName1" type="string"/>
    <aura:attribute name="showLoadingSpinner" type="boolean" default="false" />
    <aura:attribute name="fromNew" type="Boolean" default="false"/>
    <aura:attribute name="uploadedFiles" type="Integer" default="0"/>
    <aura:attribute name="uploadedFiles1" type="Integer" default="0"/>
    
    <aura:attribute name="tissue" type="Tissue_Evaluation__c"/>
    <aura:attribute name="Recovery" type="Recovery__c"/>
    <aura:registerEvent name="GetTissueDetail" type="c:GetTissueDetail"/>
    <aura:method name="getDetails">
        <aura:attribute name="MedId" type="String"/>
        <aura:attribute name="DonorId" type="String"/>
        <aura:attribute name="DonorName" type="String"/>
        <aura:attribute name="DonorAge" type="String"/>
        <aura:attribute name="DonorGender" type="String"/>
        <!--<aura:attribute name="Recoveryintent" type="String"/> -->
        <aura:attribute name="DeathDateTime" type="Datetime"/>
        <aura:attribute name="TissueImported" type="String"/>
        <aura:attribute name="recoveryId" type="string"/>
        <aura:attribute name="eyeType" type="string"/>
        <aura:attribute name="dislpayNext" type="Boolean"/>
    </aura:method>
    <aura:attribute name="MedId" type="String"/>
    <aura:attribute name="DonorId" type="String"/>
    <aura:attribute name="DonorName" type="String"/>
    <aura:attribute name="DonorAge" type="String"/>
    <aura:attribute name="DonorGender" type="String"/>
<!--    <aura:attribute name="Recoveryintent" type="String"/> -->
    <aura:attribute name="DeathDateTime" type="Datetime"/>
    <aura:attribute name="TissRecDateTime" type="Datetime"/>
    <aura:attribute name="TissueImported" type="String"/>
    <aura:attribute name="recoveryId" type="string"/>
    <aura:attribute name="eyeType" type="string"/>
    <aura:attribute name="recId" type="string"/>
    <aura:attribute name="enabled" type="Boolean" default="false"/>
    <aura:attribute name="dislpayNext" type="Boolean"/>
    <aura:attribute name="Med" type="Medical_Review__c"/>
    <aura:attribute name="Rec" type="Recovery__c"/>
    <aura:attribute name="recordId" type="Id"/>
    <aura:attribute name="tissueList" type="List"/>
    <aura:attribute name="currentUser" type="sObject"/>
    <aura:handler event="c:Imagehandler" action="{!c.handleuploadedImage}" />
    <aura:handler name="init" action="{!c.doInit}" value="{!this}"/>
    <aura:attribute name="userInfo" type="String"/>
    <aura:attribute name="buttonDisable" type="boolean" default="false"/>
    <aura:attribute name="DisableField" type="boolean" default="false"/>
    <aura:attribute name="DisableField1" type="boolean" default="false"/>
    <aura:attribute name="DisableField2" type="boolean" default="false"/>
    <aura:attribute name="DisableField3" type="boolean" default="false"/>
    <aura:attribute name="DisableField4" type="boolean" default="false"/>
    <aura:attribute name="outcomevalue" type="boolean" default="false"/>
     
    <aura:if isTrue="{!v.spinner}">
        <lightning:spinner alternativeText="Loading" size="large" />
    </aura:if>  
  
    
    <!--LOOKUP TAB INDEX-->
    <aura:handler name="oSelectedRecordEvent" event="c:selectedsObjectRecordEvent" action="{!c.focusToNextField}"/>
    
    <div class="slds-container--large slds-align_absolute-center">
        <div class="slds-grid slds-wrap slds-size_1-of-1">
            <div class="slds-size_1-of-1" style="background:#005fb2;color:#fff;">
                
                <lightning:layoutItem padding="horizontal-small">
                    
                    <div class="page-section page-header slds-p-around_x-small">
                        <h1 class="slds-text-heading--label" style="color:#fff;">TISSUE EVALUATION : INFORMATION</h1>
                        <h3 class="slds-text-heading--large">New Tissue Evaluation </h3>
                    </div> 
                </lightning:layoutItem>
            </div> 
            
            
            <!-- HEADER SECTION CONTAINING IMPORTANT INFORMATION -->               
            <div class="slds-grid slds-size_1-of-1 slds-m-bottom_small slds-m-top_small" style="border-bottom:1px solid #ccc;background:#f3f3f2;padding:10px 0px;">
                <div class="slds-col slds-size_3-of-12">
                    <div class="slds-m-bottom_xxx-small slds-grid slds-gutters">
                        <div class="slds-col slds-size_4-of-12 slds-text-align_right slds-p-around_none"><span>Donor ID</span>: </div>
                        <div class="slds-col slds-size_8-of-12">   <ui:outputText value="{!v.tisEval.Donor_ID__c}"/></div>
                        
                    </div></div>
                <div class="slds-col slds-size_2-of-12">
                    <div class="slds-m-bottom_xxx-small slds-grid slds-gutters">
                        <div class="slds-col slds-size_4-of-12 slds-text-align_right slds-p-around_none"><span>Donor Name</span>: </div>
                        <div class="slds-col slds-size_8-of-12">   <ui:outputText value="{!v.DonorName}"/></div>
                        
                    </div></div>
                
                <div class="slds-col slds-size_2-of-12">
                    <div class="slds-m-bottom_xxx-small slds-grid slds-gutters">
                        <div class="slds-col slds-size_4-of-12 slds-text-align_right slds-p-around_none"><span>Date/Time of Death</span><span style="color:#ff0000;font-weight:bold;">*</span>: </div>
                        <div class="slds-col slds-size_6-of-12">   <ui:outputDateTime format="dd/MM/yyyy HH:mm:ss" aura:id="dt"
                                                                                                value="{!v.DeathDateTime}"/></div>
                    </div></div>
                
                <div class="slds-col slds-size_1-of-12">
                    <div class="slds-m-bottom_xxx-small slds-grid slds-gutters">
                        <div class="slds-col slds-size_2-of-12 slds-text-align_right slds-p-around_none"><span>Age</span>: </div>
                        <div class="slds-col slds-size_10-of-12">   <ui:outputText value="{!v.DonorAge}"/></div>
                    </div></div>
                
                <div class="slds-col slds-size_2-of-12">
                    <div class="slds-m-bottom_xxx-small slds-grid slds-gutters">
                        <div class="slds-col slds-size_3-of-12 slds-text-align_right slds-p-around_none"><span>Gender</span>: </div>
                        <div class="slds-col slds-size_9-of-12">   <ui:outputText value="{!v.DonorGender}"/></div>
                    </div></div>
                <div class="slds-col slds-size_2-of-12">
                	<div class="slds-m-bottom_xxx-small slds-grid slds-gutters">
                        <div class="slds-col slds-size_3-of-12 slds-text-align_right slds-p-around_none"><span>Branch</span>: </div>
                        <div class="slds-col slds-size_9-of-12">   <ui:outputText value="{!v.Med.Branch_Name__c}"/></div>
                    </div>
                </div>
                  
                
                
            </div>
            <!-- HEADER SECTION END -->
            
            <div >
                <div class="slds-grid slds-wrap">  
                    
                    <!-- TISSUE DETAILS -->
                    <div class="slds-col slds-size_1-of-1">  
                        <div class="slds-page-header" style="cursor: pointer;" onclick="{!c.section01}">
                            <section class="slds-clearfix">
                                <div class="slds-float--left ">
                                    <lightning:icon class="slds-show" aura:id="One" iconName="utility:add" size="x-small" alternativeText="Indicates add"/>
                                    <lightning:icon class="slds-hide" aura:id="One" iconName="utility:dash" size="x-small" alternativeText="Indicates dash"/>
                                </div>
                                <div class="slds-m-left--large">Tissue Details</div>
                            </section>
                        </div>
                        
                        
                        <div class="slds-hide slds-p-around--medium" aura:id="One">
                            <div class="slds-grid slds-gutters slds-form slds-form_stacked slds-wrap">
                                <div class="slds-col slds-size_6-of-12">
                                    
                                    <div class="slds-form-element">
                                        <lightning:Select aura:id="eye" label="Eye" value="{!v.tisEval.Eye__c}" disabled="{!v.enabled}" required="true">
                                            <option value="">None</option>
                                            <option value="OD">OD</option>
                                            <option value="OS">OS</option>
                                        </lightning:Select>
                                    </div> 
                                    
                                    <div class="slds-form-element">
                                        <lightning:Select aura:id="tissueType" label="Tissue Type" required="true" value="{!v.tisEval.Tissue_Type__c}" onchange="{!c.ontissueTypePick}">
                                            <option value="">None</option>
                                            <option value="Cornea">Cornea</option>
                                            <option value="Sclera">Sclera</option>
                                            <option value="Whole Globe">Whole Globe</option>
                                        </lightning:Select>
                                    </div>          
                                       <!-- added by sandeep-->
                                    <aura:if isTrue="{!v.tisEval.Tissue_Type__c =='Sclera'}">
                                             <div class="slds-form-element">
                                        <lightning:Select  label="Quarter" required="false" value="{!v.tisEval.Quarter__c}">
                                            <option value="1">1</option>
                                            <option value="2">2</option>
                                            <option value="3">3</option>
                                            <option value="4">4</option>
                                            <option value="5">5</option>
                                            <option value="6">6</option>
                                            <option value="7">7</option>
                                            <option value="8">8</option>
                                        </lightning:Select>
                                    </div>   
									</aura:if>
                                    <div class="slds-form-element">
                                        <lightning:Select aura:id="tissueSubType" label="Tissue Sub-Type" required="true" value="{!v.tisEval.Tissue_Sub_Type__c}">
                                            <option value="">None</option>
                                            <option value="Whole">Whole</option>
                                            <option value="Half">Half</option>
                                            <option value="Quarter">Quarter</option>
                                            <option value="Part">Part</option>
                                        </lightning:Select>
                                    </div>           
                                    
                                    <div class="slds-form-element">
                                        <lightning:Select aura:id="procedure" label="Procedures under LFH" value="{!v.tisEval.Was_cornea_processed_under_a_LFH__c}" onchange="{!c.onProcedurePick}">
                                            <option value="None">None</option>
                                            <option value="No">No</option>
                                            <option value="Cornea Processed under LFH">Cornea Processed under LFH</option>
                                            <option value="Cornea Shifted to Another Media">Cornea Shifted to Another Media</option>
                                            <option value="Other Open Container Procedures">Other Open Container Procedures</option>
                                        </lightning:Select>
                                    </div>  
                                    
                                    <div aura:id="TissueProcessedBy" class="slds-show">
                                        <c:customLookup contactRole="specularTechTiss" objectAPIName="Contact" IconName="standard:contact" selectedRecord="{!v.DonorCon4}" label="Tissue Processed By"  objName="contact"/>
                                    </div>
                                    
                                    <div class="slds-form-element">
                                        <lightning:Select aura:id="PreservMedium" label="Preservation Medium" value="{!v.tisEval.Preservation_Medium__c}" disabled="{!v.ProcedureNo}" onchange="{!c.tissueExpiryCalc}">
                                            <option value="None">None</option>
                                            <option value="MK">MK</option>
                                            <option value="Cornisol">Cornisol</option>
                                            <option value="OptisolGS">OptisolGS</option>
                                            <option value="Eusol-c">Eusol-c</option>
                                            <option value="Life4c">Life4c</option>
                                            <option value="100%EtOH">100%EtOH</option>
                                            <option value="95%EtOH">95%EtOH</option>
                                            <option value="70%EtOH">70%EtOH</option>
                                            <option value="MoistChamber">MoistChamber</option>
                                            <option value="Glycerin">Glycerin</option>
                                        </lightning:Select>
                                    </div> 
                                    
                                    <div class="slds-form-element ">
                                        <lightning:input type="string" label="Media Lot #" value="{!v.tisEval.Media_Lot__c}" disabled="{!v.ProcedureNo}"/>             
                                    </div>
                                    
                                    <div class="slds-form-element ">
                                        <lightning:input type="datetime" label="Media Expiry Date" value="{!v.tisEval.Media_Expiration_Date__c}" disabled="{!v.ProcedureNo}"/>             
                                    </div> 
                                    
                                    <div class="slds-form-element ">
                                        <lightning:input aura:id="tissueExpiry" type="datetime" label="Tissue Expiry Date" value="{!v.tisEval.Tissue_Expiry_Date__c}" />             
                                    </div> 
                                    
                                </div>
                                
                                
                                <div class="slds-col slds-size_6-of-12">
                                    
                                    <div class="slds-form-element">
                                        <lightning:input type="string" label="Donor ID" value="{!v.tisEval.Donor_ID__c}" required="true" disabled="true"/>             
                                    </div>
                                    
                                    <div class="slds-form-element ">
                                        <lightning:input type="string" label="Tissue ID" value="{!v.tisEval.Tissue_ID__c}" disabled="true"/>             
                                    </div>
                                    
                                    <div class="slds-form-element">
                                        
                              <!--  <lightning:input type="string" label="Recovery Intent" value="{!v.Recoveryintent}" disabled= "true" />     -->       
                       
                                      
                                 <lightning:Select label="Recovery Intent" value="{!v.tisEval.Recovery_Intent__c}" disabled="true">
                                            <option value="None">None</option>
                                            <option value="Transplant">Transplant</option>
                                            <option value="Research">Research</option>
                                            <option value="Training">Training</option>
                                        </lightning:Select> 
                                        
                                    </div>  
                                    
                                </div>
                            </div>
                        </div> </div>
                    <!-- TISSUE DETAILS END -->
                    
                    
                    <!-- SPECULAR EVALUATION -->
                    <div class="slds-col slds-size_1-of-1">           
                        <div class="slds-page-header"  style="cursor: pointer;" onclick="{!c.section02}">
                            <section class="slds-clearfix">
                                <div class="slds-float--left"> 
                                    <lightning:icon class="slds-show" aura:id="Two" iconName="utility:add" size="x-small" alternativeText="Indicates add"/>
                                    <lightning:icon class="slds-hide" aura:id="Two" iconName="utility:dash" size="x-small" alternativeText="Indicates dash"/>
                                </div>
                                <div class="slds-m-left--large">Specular Evaluation</div>
                            </section>
                        </div>
                        
                        <div aura:id="Two" class="slds-hide slds-p-around--medium"> 
                            <div class="slds-grid slds-gutters slds-form slds-form_stacked slds-wrap">
                                
                                <div class="slds-col slds-size_1-of-1">   
                                    <div class="slds-form-element" style="width:30%;">
                                        <lightning:Select aura:id="SpecEval" value="{!v.tisEval.Specular_Evaluation_Done__c}" onchange="{!c.OnSpecPick}" name="SpecEval" label="Specular Evaluation Done">
                                            <option value="No">No</option>
                                            <option value="Yes">Yes</option>
                                        </lightning:Select>
                                    </div>  </div> </div>
                            
                            <div class="slds-grid slds-gutters slds-form slds-form_stacked slds-wrap">      
                                <aura:if isTrue="{!v.display}">
                                    
                                    <div class="slds-col slds-size_6-of-12">             
                                        
                                        
                                        <div>
                                            <c:customLookup contactRole="specularTech1" objectAPIName="Contact" IconName="standard:contact" selectedRecord="{!v.DonorCon}" label="Specular Evaluation Done By"  objName="contact"/>
                                        </div>
                                        
                                        <div class="slds-form-element ">
                                            <lightning:input aura:id="SpecularEvaluationDateTime" type="datetime" label="Specular Evaluation Date/Time" value="{!v.tisEval.Specular_Evaluation_Date_Time__c}" onchange="{!c.TissRecDate}" />             
                                        <aura:if isTrue="{!v.booleanvalue3}">
                                            <div class="slds-text-color_error slds-p-left_x-small">
                                                Selected Date/Time Should be Greater than Specular Evaluation Date/Time.
                                            </div>
                                        </aura:if>
                                        </div>  
                                        
                                        
                                        <div class="slds-grid">
                                        <div class="slds-form-element slds-size_5-of-12">
                                            <label class="slds-form-element__label" for="inputName"><b>Upload Specular Image</b></label>
                                            <!-- <c:ImageUploadcmp/>-->
                                            <!--<c:fileUpload aura:id="fileUP" fromNew="true" firstimg="true" imageName="specEval"/>-->
                                            <lightning:input aura:id="fileId" onchange="{!c.handleFilesChange}" type="file" name="specEval" label="Upload Attachment" multiple="false"/>
                                            <div class="slds-text-body_small slds-text-color_brand">{!v.fileNamespecEval} </div>
                                            <!--<aura:if isTrue="{!v.fromNew}">
                                            	<lightning:button variant="brand" label="Save Attachment" title="specEval Save" onclick="{!c.doSave}" name="specEvalSave"/>
                                            </aura:if>-->
                                        </div>
                                    </div> 
                                        </div>
                                    
                                    
                                    <div class="slds-col slds-size_6-of-12">
                                        
                                        <div class="slds-form-element">
                                            <lightning:input type="Integer" label="Cell Count (per mm2)" value="{!v.tisEval.Cell_Count_per_mm2__c}"/>             
                                        </div>  
                                        
                                        <div class="slds-form-element ">
                                            <lightning:input type="Integer" label="Number of Fields" value="{!v.tisEval.Number_of_Fields__c}"/>             
                                        </div>  
                                        
                                        <div class="slds-form-element ">
                                            <lightning:input type="Integer" name="Num" label="Pachymetry (microns)" value="{!v.tisEval.Pachymetry_microns__c}"/>             
                                        </div> 
                                        
                                        <div class="imagecontainer slds-form-element" style="text-align: center;">
                                            <img class="image" src="{!v.imgurl}" aura:id="imagePreview" />                                            
                                        </div>
                                        
                                    </div>
                                    
                                    
                                </aura:if> </div>
                            
                        </div>     
                    </div>
                    <!-- SPECULAR EVALUATION END --> 
                    
                    
                    <!-- SLIT LAMP EVALUATION -->
                    <div class="slds-col slds-size_1-of-1">
                        <div class="slds-page-header" style="cursor: pointer;" onclick="{!c.section03}">
                            <section class="slds-clearfix">
                                <div class="slds-float--left">
                                    <lightning:icon class="slds-show" aura:id="Three" iconName="utility:add" size="x-small" alternativeText="Indicates add"/>
                                    <lightning:icon class="slds-hide" aura:id="Three" iconName="utility:dash" size="x-small" alternativeText="Indicates dash"/>
                                </div>
                                <div class="slds-m-left--large">Slit Lamp Evaluation</div>
                            </section>
                        </div> 
                        
                        <div class="slds-hide slds-p-around--medium" aura:id="Three">
                            
                            <div class="slds-grid slds-gutters slds-form slds-form_stacked slds-wrap">                   
                                <div class="slds-col slds-size_1-of-1">
                                    <div class="slds-form-element" style="width:30%;">
                                        <lightning:Select aura:id="Slitval" value="{!v.tisEval.Slit_Lamp_Done__c}" onchange="{!c.OnSlitPick}" name="Slitval" label="Slit Lamp Done">
                                            <option value="No">No</option>
                                            <option value="Yes">Yes</option>
                                        </lightning:Select>
                                    </div> 
                                    
                                    
                                    <aura:if isTrue="{!v.display1}">
                                        
                                        <!-- EPITHELIUM -->
                                        <div class="slds-page-header"  style="cursor: pointer;" onclick="{!c.section04}">
                                            <section class="slds-clearfix">
                                                <div class="slds-float--left"> 
                                                    <lightning:icon class="slds-show" aura:id="Four" iconName="utility:add" size="x-small" alternativeText="Indicates add"/>
                                                    <lightning:icon class="slds-hide" aura:id="Four" iconName="utility:dash" size="x-small" alternativeText="Indicates dash"/>
                                                </div>
                                                <div class="slds-m-left--large">Epithelium</div>
                                            </section>
                                        </div>
                                        
                                        <div aura:id="Four" class="slds-hide slds-p-around--medium"> 
                                            
                                            <div class="slds-grid slds-gutters slds-form slds-form_stacked slds-wrap">
                                                <div class="slds-col slds-size_6-of-12">
                                                    
